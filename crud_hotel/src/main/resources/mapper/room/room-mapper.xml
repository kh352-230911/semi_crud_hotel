<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="room">
    <select id="findByRoomNum" resultType="room">
        select
            *
        from
            tb_room
        where
            room_num = #{roomNum}
    </select>

</mapper>